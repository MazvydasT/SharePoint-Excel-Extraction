{
	"name": "sharepoint-excel-extraction",
	"version": "1.13.0",
	"description": "",
	"author": "",
	"private": false,
	"license": "MIT",
	"scripts": {
		"generate-schema": "npx ts-json-schema-generator --path src/configuration/interfaces/IJSONConfig.ts -o src/schemas/config.schema.json",
		"prebuild": "rimraf dist && npm run generate-schema",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@google-cloud/bigquery": "^7.3.0",
		"@nestjs/axios": "^3.0.0",
		"@nestjs/cache-manager": "^2.1.0",
		"@nestjs/common": "^10.2.7",
		"@nestjs/core": "^10.2.7",
		"@nestjs/platform-express": "^10.2.7",
		"axios": "^1.5.1",
		"axios-ntlm": "^1.4.1",
		"cache-manager": "^5.2.4",
		"commander": "^11.0.0",
		"cron-parser": "^4.9.0",
		"dotenv": "^16.3.1",
		"fast-glob": "^3.3.1",
		"https-proxy-agent": "^7.0.2",
		"ix": "^5.0.0",
		"moment": "^2.29.4",
		"node-sp-auth": "^3.0.7",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^5.0.5",
		"rxjs": "^7.8.1",
		"xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.1.18",
		"@nestjs/schematics": "^10.0.2",
		"@nestjs/testing": "^10.2.7",
		"@types/cache-manager": "^4.0.3",
		"@types/express": "^4.17.19",
		"@types/jest": "29.5.5",
		"@types/node": "^20.8.4",
		"@types/supertest": "^2.0.14",
		"@typescript-eslint/eslint-plugin": "^6.7.5",
		"@typescript-eslint/parser": "^6.7.5",
		"eslint": "^8.51.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.1",
		"jest": "29.7.0",
		"prettier": "^3.0.3",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.3",
		"ts-jest": "29.1.1",
		"ts-json-schema-generator": "^1.3.0",
		"ts-loader": "^9.5.0",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "4.2.0",
		"typescript": "^5.2.2"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
